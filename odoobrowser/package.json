{
  "name": "odoobrowser",
  "displayName": "OdooBrowser",
  "description": "Browser Odoo Models, XmlIds",
  "repository": {
    "url": "https://git.clear-consulting.de/marc/vscodeextension_browserodoo"

  },
  "version": "0.0.3",
  "publisher": "Marc-ChristianWimmer",
  "engines": {
    "vscode": "^1.54.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": ["*"],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "odoobrowser.showXmlIds",
        "title": "Odoo Show XmlIds"
      },
      {
        "command": "odoobrowser.updateXmlIds",
        "title": "Odoo Update XmlIds"
      },
      {
        "command": "odoobrowser.byeWorld",
        "title": "Good Bye World"
      },
      {
        "command": "odoobrowser.Goto",
        "title": "Godoo [Goto]"
      },
      {
        "command": "odoo_debugcommand.update_module",
        "title": "Odoo Debug: Flag File Update Module"
      },
      {
        "command": "odoo_debugcommand.run_unittest",
        "title": "Odoo Debug: Run Unit Test"
      },
      {
        "command": "odoo_debugcommand.restart",
        "title": "Odoo Debug: Restart"
      },
      {
        "command": "odoo_debugcommand.update_view",
        "title": "Odoo Debug: Update View"
      },
      {
        "command": "odoo_debugcommand.update_ast_current_file",
        "title": "Odoo Debug: Update Ast Current File"
      },
      {
        "command": "odoo_debugcommand.update_ast_all",
        "title": "Odoo Debug: Update Ast All"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^7.1.3",
    "@types/line-reader": "^0.0.33",
    "@types/mocha": "^8.0.4",
    "@types/node": "^12.11.7",
    "@types/vscode": "^1.54.0",
    "@types/xmldom": "^0.1.30",
    "@typescript-eslint/eslint-plugin": "^4.14.1",
    "@typescript-eslint/parser": "^4.14.1",
    "eslint": "^7.19.0",
    "glob": "^7.1.6",
    "mocha": "^8.2.1",
    "typescript": "^4.1.3",
    "vscode-test": "^1.5.0"
  },
  "dependencies": {
    "async-line-reader": "^1.0.0",
    "fast-xml-parser": "^3.19.0",
    "line-reader": "^0.4.0",
    "python-shell": "^2.0.3",
    "web-request": "^1.0.7",
    "xmldom": "^0.5.0",
    "xmldom-ts": "^0.3.1",
    "xpath": "^0.0.32"
  }
}
